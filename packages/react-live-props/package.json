{
  "name": "react-live-props",
  "version": "0.0.1",
  "description": "Live playground for interacting with props on React components in MDX files.",
  "author": "Hydrate <dev@hydrate.io> (https://hydrate.io)",
  "license": "MIT",
  "repository": "hydrateio/react-live-props",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "babel src --out-dir dist --ignore \"src/**/*test*\" --ignore \"src/test.js\" --source-maps",
    "start": "babel src --watch --out-dir dist --ignore \"src/**/*test*\" --ignore \"src/test.js\" --source-maps",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^2.0.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/generator": "^7.1.3",
    "@babel/parser": "^7.1.3",
    "@babel/traverse": "^7.1.4",
    "@babel/types": "^7.1.3",
    "classnames": "^2.2.6",
    "json-schema-faker": "^0.5.0-rc16",
    "react-docgen-to-json-schema": "^0.1.0",
    "react-jsonschema-form": "^1.0.4",
    "styled-components": "^4.0.0",
    "unist-util-visit": "^1.4.0"
  }
}
